{
  "name": "socionics-research-ipdb",
  "version": "1.0.0",
  "description": "Integrated Personality Database for socionics research with DuckDB, vector search, and multi-language API support",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "echo 'Build not required - using JavaScript files directly'",
    "dev": "echo 'Development mode - files ready'",
    "test": "node --test",
    "test:python": "python comprehensive_test.py",
    "demo": "node ipdb/simple_server.cjs",
    "demo:python": "python ipdb/demo.py",
    "api": "node ipdb/simple_server.cjs",
    "start": "node ipdb/simple_server.cjs"
  },
  "keywords": [
    "socionics",
    "personality-database",
    "psychology",
    "mbti",
    "duckdb",
    "vector-search",
    "machine-learning"
  ],
  "author": "Socionics Research Team",
  "license": "MIT",
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.28.0",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.6.2"
  },
  "optionalDependencies": {
    "duckdb": "^1.1.3",
    "hnswlib-node": "^3.0.0",
    "hnswlib-wasm": "^0.8.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./python": {
      "import": "./ipdb/database_manager.py"
    },
    "./wasm": {
      "import": "./dist/wasm.js",
      "types": "./dist/wasm.d.ts"
    }
  }
}
