{
  "name": "socionics-research-ipdb",
  "version": "1.0.0",
  "description": "Integrated Personality Database for socionics research with DuckDB, vector search, and multi-language API support",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon ipdb/wikia_server.cjs",
    "build": "node ipdb/ingest_parquet_data.cjs",
    "serve": "node ipdb/wikia_server.cjs",
    "start": "node ipdb/wikia_server.cjs",
    "test": "node --test",
    "ingest": "node ipdb/ingest_parquet_data.cjs",
    "demo": "node ipdb/wikia_server.cjs",
    "api": "node ipdb/wikia_server.cjs"
  },
  "keywords": [
    "socionics",
    "personality-database",
    "psychology",
    "mbti",
    "duckdb",
    "vector-search",
    "machine-learning"
  ],
  "author": "Socionics Research Team",
  "license": "MIT",
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.28.0",
    "cors": "^2.8.5",
    "duckdb": "^1.1.3",
    "express": "^4.18.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.0",
    "nodemon": "^3.1.4",
    "playwright": "^1.55.0",
    "typescript": "^5.6.2"
  },
  "optionalDependencies": {
    "hnswlib-node": "^3.0.0",
    "hnswlib-wasm": "^0.8.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./wasm": {
      "import": "./dist/wasm.js",
      "types": "./dist/wasm.d.ts"
    }
  }
}
